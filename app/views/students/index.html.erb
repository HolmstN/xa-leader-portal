<h1>All Students</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>
  
  <% @students.each do |student| %>
    <tr>
      <td><%= student.name %></td>
      <td><%= student.email %></td>
      <td><%= student.phone %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
    </tr>
    <% end %>
</table>

